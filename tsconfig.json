{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "importHelpers": true,
    "types": ["@types/node", "@nuxt/types", "nuxt-graphql-middleware"]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "modules/simple",
    "modules/nuxt-graphql-middleware",
    "backup"
  ],
  "vueCompilerOptions": {
    "experimentalCompatMode": 2
  }
}
